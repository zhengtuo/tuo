<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

        <variable
            name="title"
            type="String" />

    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".ui.learn.view.GetResultActivity">

        <include
            android:id="@+id/toolbar"
            layout="@layout/layout_toolbar"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:hideLine="@{true}"
            app:title="@{title}" />

        <com.zheng.tuo.utils.IconFontView
            android:id="@+id/ic_icon"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/e766"
            android:textSize="@dimen/sp_40"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/tv_back"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="点击返回数据"
            app:layout_constraintLeft_toLeftOf="@+id/ic_icon"
            app:layout_constraintRight_toRightOf="@+id/ic_icon"
            app:layout_constraintTop_toBottomOf="@+id/ic_icon" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>